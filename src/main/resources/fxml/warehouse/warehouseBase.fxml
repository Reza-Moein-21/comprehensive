<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="ir.comprehensive.controller.warehouse.WarehouseBaseController"
      xmlns="http://javafx.com/javafx" nodeOrientation="RIGHT_TO_LEFT"
      AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0"
      AnchorPane.leftAnchor="0">

    <TabPane VBox.vgrow="ALWAYS">
        <Tab fx:id="tabWarehouse" text="%warehouse" style="-fx-font-weight: bold" closable="false">
            <fx:include source="warehouse.fxml"/>
        </Tab>
        <Tab fx:id="tabWarehouseCategories" text="%warehouseCategory" style="-fx-font-weight: bold" closable="false">
            <fx:include source="warehouseCategory.fxml"/>
        </Tab>

    </TabPane>

</VBox>
